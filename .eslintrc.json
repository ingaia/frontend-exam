{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": 0,
        "consistent-return": 0,
        "no-multi-assign": 0,
        "no-unused-expressions": 0,
        "no-confusing-arrow": 0,
        "import/prefer-default-export": [0],
        "object-curly-newline": 0,
        "no-shadow": 0,
        "react/jsx-filename-extension": [0],
        "react/jsx-props-no-spreading": [0]
    }
}
